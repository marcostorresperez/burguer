<odoo>
    <data>
        <!-- PRODUCTOR -->
        <record model="ir.ui.view" id="burguer.productortree">
            <field name="name">Productor</field>
            <field name="model">burguer.productor</field>
            <field name="arch" type="xml">
                <tree decoration-info="template==True">
                    <field name="name"/>
                    <field name="produccion"/>
                    <field name="recurso"/>
                    <field name="nivel"/>
                    <field name="description"/>
                    <field name="nombre_jugador"/>
                    <field name="image_small" widget="image"/>
                    <field name="template"/>

                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="burguer.productorform">
            <field name="name">Productor form</field>
            <field name="model">burguer.productor</field>
            <field name="arch" type="xml">
                <form>
                    <header style="background:#7C7BAD">
                        <field style="float:left;color:white;font-size:25px;margin-right:50%;padding: 10px"
                               name="name"/>
                    </header>
                    <sheet>
                        <group>

                            <field name="produccion"/>
                            <field name="recurso"/>
                            <field name="nivel"/>
                            <field name="description"/>
                            <field name="image_small" widget="image"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record model="ir.actions.act_window" id="burguer.action_productor">
            <field name="name">Productor</field>
            <field name="res_model">burguer.productor</field>
            <field name="view_mode">tree,form,kanban</field>
        </record>
        <record model="ir.ui.view" id="burguer.productor_kanban_view">
            <field name="name">Kanban Productor</field>
            <field name="model">burguer.productor</field>
            <field name="arch" type="xml">
                <kanban>
                    <field name="image_small"/>
                    <field name="id"/>
                    <field name="name"/>
                    <field name="produccion"/>
                    <field name="recurso"/>
                    <field name="template"/>
                    <field name="nivel"/>
                    <field name="description"/>
                    <field name="color"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div t-attf-class="
                            oe_kanban_card
                            oe_kanban_global_click
                            o_kanban_record_has_image_fill
                            #{kanban_color(record.color.raw_value)}">
                                <div class="oe_burguer_kanban_div_image">
                                    <img class="oe_kanban_image"
                                         t-att-alt="record.name.value"
                                         t-att-src="kanban_image('burguer.productor', 'image_small', record.id.value)"/>
                                    <button name="level_up" type="object" string="Nivel"
                                            class="oe_burguer_kanban_button">Subir Nivel
                                    </button>
                                    <button name="level_cero" type="object" string="Nivel"
                                            class="oe_burguer_kanban_button">Restaurar
                                    </button>
                                </div>
                                <div class="oe_kanban_details">
                                    <a type="open">
                                        <h4>
                                            <field name="name"/>
                                        </h4>
                                    </a>
                                    <ul>
                                        <li>Nivel:
                                            <field name="nivel"/>
                                        </li>
                                        <li>Nombre:
                                            <field name="name"/>
                                        </li>
                                        <li>Produccion:
                                            <field name="produccion"/>
                                        </li>
                                        <li>Descripcion:
                                            <field name="description"/>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>


        <!-- ALMACEN -->
        <record model="ir.ui.view" id="burguer.almacen_tree">
            <field name="name">Almacen</field>
            <field name="model">burguer.almacen</field>
